<div class="page">
<h1 class="entry-title">Entries</h1>
<ol class="ol-entry"><% @entries.each do |entry| %>
  <li class="entry-li">
  <h1 class="entry-prompt"> <%= entry.prompt.prompt_content %> </h1>
  <h2 class="entry-response"> <%= entry.response %> </h2>
</li>
<% end %></ol>
<h3 class="link"><%= link_to 'New Entry Form', new_entry_path %></h3>
</div>
